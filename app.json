{
  "expo": {
    "name": "walletapp",
    "slug": "walletapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "walletapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "NSPhotoLibraryUsageDescription": "We need access to your photos to select a profile image.",
      "bundleIdentifier": "com.anonymous.walletapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": ["READ_MEDIA_IMAGES", "READ_MEDIA_IMAGES"]
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.walletapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "hideAppName": true,
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
